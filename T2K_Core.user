    <link linkage="attribute" thing="attrStr"/>
    </thing>
  <thing id="skT2KDriving" name="Driving" description="Use the DRIVING skill to maneuver motorcycles, cars, armored personnel carriers and even tanks. Specialties apply to specific types of vehicles." compset="Skill">
