  <thing id="spcT2KMartialArt" name="Martial Artist" description="Your close combat attacks have a crit threshold of 3 instead of 4." compset="Specialty" uniqueness="useronce">
    <usesource source="srcT2K"/>
    <tag group="User" tag="Activation"/>
    <eval phase="PreTraits" priority="5000">var myVal as string
var newVal as number

perform hero.childfound[wpUnarmed].setfocus
myVal = focus.field[wpCrit].text
newVal = myVal - 1

focus.field[wpCrit].text = newVal</eval>
    </thing>
