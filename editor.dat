<?xml version="1.0" encoding="UTF-8"?>

<!-- This file contains the definitions of how the Editor should handle all the
      various publicly accessible objects (i.e. component sets) within the game
      system. A separate "editthing" elemenet is defined for each component set,
      along with the appropriate details for how to configure each facet via
      "inputthing" elements.
-->

<document signature="Hero Lab Data">

  <!-- Inputthing groups -->
  <inputthinggroup
    id="SourceInfo">
	<inputthing
      name="Source Info"
      helptext="Entries related to the source material are below.">
      <it_separator/>
      </inputthing>
    </inputthinggroup>
	
  <inputthinggroup
    id="Consumables">
	<inputthing
      name="Consumables"
      helptext="Entries related to Consumables are below.">
      <it_separator/>
      </inputthing>
    <inputthing
      name="Has Consumables?"
      helptext="Check is the item has one or more consumable values?">
      <it_tagcheck group="Helper" tag="Consumable" default="yes"/>
      </inputthing>
    <inputthing
      name="Air Supply"
      helptext="Specify the Air Supply rating for the Item.">
      <it_field field="grAir"/>
      </inputthing>
    <inputthing
      name="Food Supply"
      helptext="Specify the Food Supply rating for the Item.">
      <it_field field="grFood"/>
      </inputthing>
    <inputthing
      name="Power Supply"
      helptext="Specify the Air Supply rating for the Item.">
      <it_field field="grPower"/>
      </inputthing>
    <inputthing
      name="Water Supply"
      helptext="Specify the Water Supply rating for the Item.">
      <it_field field="grWater"/>
      </inputthing>
	<inputthing
	  name="-">
      <it_separator/>
      </inputthing>
    </inputthinggroup>

  <!-- Archetype editthing -->
  <editthing
    compset="Archetype"
    name="Archetype"
    prefix="arc"
    description="Archetypes have no special fields - only a name. If a race confers an ability,{br}create it as a special ability and bootstrap it from the race."
    summary="Defines a Archetype that can be selected by a character.">
    <inputthing
      name="Primary/Secondary Archetype?"
      helptext="Specify whether this is a Primary or Secondary Archetype.">
	  <it_tagpick group="ArchetypeCat" tag="?" />
      </inputthing>

    <inputthing
      name="Valid Branches"
      helptext="Specify which branches are available to this archetype.">
      <it_taglist group="ArchetypeBranches" tag="?" />
      </inputthing>
	  
    <inputthing
      name="Key Attribute"
      helptext="Specify any key attribute for the Archetype">
      <it_tagpick group="KeyAttribute" tag="?" />
      </inputthing>
    <inputthing
      name="Key Skill"
      helptext="Specify any key skills for the Archetype">
      <it_taglist group="KeySkill" tag="?" />
      </inputthing>
    <inputthing
      name="Talents"
      helptext="Details about Primary and Secondary archetypes">
      <it_separator/>
      </inputthing>
    <inputthing
      name="Granted Talents"
      helptext="Select any Talents provided by the Archetype">
      <it_bootcustom compset="Talent">
        </it_bootcustom>
      </inputthing>
    <inputthing
      name="Select Talents"
      helptext="Specify the tagexpr for selecting talents related to the Archetype.">
      <it_field field="arTalentSelect"/>
      </inputthing>	  
    <inputthing
      name="Number of Talents"
      helptext="Specify the number of talents to select (if any).">
      <it_field field="acArchNumTalents"/>
      </inputthing>	
    <inputthing>
      <it_groupref id="SourceInfo"/>
      </inputthing>
    </editthing>
	
  <!-- Attribute editthing -->
  <editthing
    compset="Branch"
    name="Archetype Branch"
    prefix="abr"
    description="."
    summary="Defines a Branch for an Archetype to select.">
    <inputthing
      name="Branch"
      helptext="Specify the Archetype Branch this is.">
      <it_tagpick group="ArchetypeBranches" tag="?" require="yes" deftag="BlueCollar"/>
      </inputthing>

    <inputthing
      name="Military Branch?"
      helptext="Is this a Military Branch?">
      <it_tagcheck group="ArchetypeBranches" tag="Military"/>
      </inputthing>
	  
    <inputthing>
      <it_groupref id="SourceInfo"/>
      </inputthing>
    </editthing>
	
  <!-- Setting Adjustments editthing -->
  <editthing
    compset="SettingAdjust"
    name="Setting Adjust"
    prefix="set"
    description="Setting-specific adjustments to starting values."
    summary="Setting-specific adjustments to starting values.">

    <inputthing
      name="Maximum Attribute Points"
      helptext="Specify the number of Attribute points added/subtracted.">
      <it_field field="setAttrPts"/>
      </inputthing>

    <inputthing
      name="Maximum Skill Points"
      helptext="Specify the number of Skill points added/subtracted.">
      <it_field field="setSkillPts"/>
      </inputthing>
	  
    <inputthing
      name="Archetypes"
      helptext="Details about Primary and Secondary archetypes">
      <it_separator/>
      </inputthing>
	  
    <inputthing
      name="Use Primary Archetypes?"
      helptext="Specify whether this campaign uses Primary Archetypes.">
      <it_tagcheck group="Hero" tag="UseArchetypePri"/>
      </inputthing>
	
	<inputthing
      name="Require Primary Archetypes?"
      helptext="Specify whether Primary Archetypes are required.">
      <it_tagcheck group="Hero" tag="ArchtypePriReq"/>
      </inputthing>

    <inputthing
      name="Primary Archetype Term"
      helptext="Specify the term to use to describe the Archetype. (i.e. Species, Guilds, Deities, etc...)">
      <it_field field="setArchTermPri"/>
      </inputthing>

    <inputthing
      name="Use Secondary Archetypes?"
      helptext="Specify whether this campaign uses a secondary form of Archetypes.">
      <it_tagcheck group="Hero" tag="UseArchetypeSec"/>
      </inputthing>

	<inputthing
      name="Require Secondary Archetypes?"
      helptext="Specify whether Secondary Archetypes are required.">
      <it_tagcheck group="Hero" tag="ArchtypeSecReq"/>
      </inputthing>
	  
    <inputthing
      name="Secondary Archetype Term"
      helptext="Specify the term to use to describe the Archetype. (i.e. Species, Guilds, Deities, etc...)">
      <it_field field="setArchTermSec"/>
      </inputthing>

    <inputthing
      name="Talents"
      helptext="Details about Talents">
      <it_separator/>
      </inputthing>	
	  
    <inputthing
      name="Ranked Talents?"
      helptext="Specify whether talents in this campaign have ranks.">
      <it_tagcheck group="Hero" tag="RankedTalents"/>
      </inputthing>
	  
    <inputthing
      name="Options"
      helptext="Details about optional content">
      <it_separator/>
      </inputthing>

    <inputthing
      name="Use Pride?"
      helptext="Specify whether this campaign uses Pride.">
      <it_tagcheck group="Hero" tag="UsePride"/>
      </inputthing>
	  
    <inputthing
      name="Use Dark Secret?"
      helptext="Specify whether this campaign uses Dark Secrets.">
      <it_tagcheck group="Hero" tag="UseDarkSecret"/>
      </inputthing>
	  
    <inputthing
      name="Use Big Dreams?"
      helptext="Specify whether this campaign uses Big Dreams.">
      <it_tagcheck group="Hero" tag="UseBigDream"/>
      </inputthing>	
	  
    <inputthing
      name="Use Agenda?"
      helptext="Specify whether this campaign uses Personal Agenda.">
      <it_tagcheck group="Hero" tag="UseAgenda"/>
      </inputthing>
	  
    <inputthing
      name="Use Moral Code?"
      helptext="Specify whether this campaign uses Moral Code.">
      <it_tagcheck group="Hero" tag="UseMoralCode"/>
      </inputthing>
	  
    <inputthing
      name="Use How Met?"
      helptext="Specify whether this campaign uses How Met.">
      <it_tagcheck group="Hero" tag="UseHowMet"/>
      </inputthing>
    <inputthing
      name="Signature Items?"
      helptext="Specify whether this campaign uses Signature Items.">
      <it_tagcheck group="Hero" tag="UseSigItems"/>
      </inputthing>
	  
    <inputthing>
      <it_groupref id="SourceInfo"/>
      </inputthing>
    </editthing>
	
  <!-- Attribute editthing -->
  <editthing
    compset="Attribute"
    name="Attribute"
    prefix="attr"
    description="Attributes are incredibly simple. They have a minimum and maximum value.{br}They also have an optional short name and/or abbreviation."
    summary="Defines an Attribute upon which starting CPs can be spent by a character.">
    <inputthing
      name="Minimum Value"
      helptext="Specify the minimum rating for the attribute (default is 0).">
      <it_field field="trtMinimum"/>
      </inputthing>
    <inputthing
      name="Maximum Value"
      helptext="Specify the maximum rating for the attribute (default is 10).">
      <it_field field="trtMaximum"/>
      </inputthing>
    <inputthing
      name="Abbreviation"
      helptext="Specify a very short abbreviation for use in displaying the attribute via its abbreviation (max 5 characters).">
      <it_field field="trtAbbrev"/>
      </inputthing>

    <inputthing>
      <it_groupref id="SourceInfo"/>
      </inputthing>
    </editthing>


  <!-- Skill editthing -->
  <editthing
    compset="Skill"
    name="Skill"
    prefix="sk"
    description="As basic traits, Skills have a minimum and maximum value, as well as{br}an optional short name and/or abbreviation."
    summary="Defines a Skill upon which starting CPs can be spent by a character.">
    <inputthing
      name="Minimum Value"
      helptext="Specify the minimum rating for the skill (default is 0).">
      <it_field field="trtMinimum"/>
      </inputthing>
    <inputthing
      name="Abbreviation"
      helptext="Specify a very short abbreviation for use in displaying the skill via its abbreviation (max 5 characters).">
      <it_field field="trtAbbrev"/>
      </inputthing>
    <inputthing
      name="Linked Attribute for Skill"
      helptext="Select the attribute used to calculate the dice pool for this skill.">
      <it_linkage compset="Attribute" linkage="attribute"/>
      </inputthing>
	<inputthing
      name="Combat Use"
      helptext="Options for using the skill in combat.">
      <it_separator/>
      </inputthing>
    <inputthing
      name="Use for Melee Combat?"
      helptext="This skill is used for determining attacks with Melee Weapons.">
      <it_tagcheck group="SkillUse" tag="Melee"/>
      </inputthing>
    <inputthing
      name="Use for Ranged Combat?"
      helptext="This skill is used for determining attacks with Ranged Weapons.">
      <it_tagcheck group="SkillUse" tag="Ranged"/>
      </inputthing>
	<inputthing
      name="Stunts"
      helptext="Stunt Information related to the skill.">
      <it_separator/>
      </inputthing>
    <inputthing
      name="Stunts"
      helptext="Specify any information regarding stunts for the Skill.">
      <it_field field="sklStuntText"/>
      </inputthing>
    <inputthing>
      <it_groupref id="SourceInfo"/>
      </inputthing>
    </editthing>

  <!-- Specialty editthing -->
  <editthing
    compset="Specialty"
    name="Specialty"
    prefix="spc"
    description="Specialty."
    summary="Defines a Specialty which can be chosen by the character.">

    <inputthing
      name="Activated by User?"
      helptext="Specify whether this special ability can be activated by the user via the In-Play tab.">
      <it_tagcheck group="User" tag="Activation"/>
      </inputthing>
    <inputthing
      name="Creation Only?"
      helptext="Specify whether this special ability can only be selected for a character during creation.">
      <it_tagcheck group="User" tag="CreateOnly"/>
      </inputthing>

    <inputthing
      name="Focus Required?"
      helptext="Specify whether this specialty requires the user to specify a focus (e.g. Language).">
      <it_tagcheck group="User" tag="NeedDomain"/>
      </inputthing>
	  
    <inputthing>
      <it_groupref id="SourceInfo"/>
      </inputthing>
    </editthing>
	
  <!-- Talent editthing -->
  <editthing
    compset="Talent"
    name="Talents"
    prefix="tal"
    description="As basic traits, talents have various characteristics that are not currently used.{br}They also have an optional custom name for display on the Special tab."
    summary="Defines a Talent upon which starting CPs can be spent by a character.">
    <inputthing
      name="Name for Special Tab"
      helptext="Specify an optional custom name to be used on the Special tab.">
      <it_field field="spcName"/>
      </inputthing>
    <inputthing
      name="Category"
      helptext="Specify what category this talent belongs to (General, Archetype, Kin, etc.).">
	  <it_tagpick group="TalentCat" tag="?" />
      </inputthing>
    <inputthing
      name="Archetype"
      helptext="Specify what archetype this talent is associated with (if any).">
	  <it_tagpick group="Archetype" tag="?" />
      </inputthing>
	  
    <inputthing
      name="Hide from User?"
      helptext="Specify whether this special ability is hidden from direct user selection (e.g. Flight is only conferred by the Avian race).">
      <it_tagcheck group="Hide" tag="Talent"/>
      </inputthing>
    <inputthing
      name="Activated by User?"
      helptext="Specify whether this special ability can be activated by the user via the In-Play tab.">
      <it_tagcheck group="User" tag="Activation"/>
      </inputthing>
    <inputthing
      name="Creation Only?"
      helptext="Specify whether this special ability can only be selected for a character during creation.">
      <it_tagcheck group="User" tag="CreateOnly"/>
      </inputthing>
	  
    <inputthing
      name="Talent Ranks"
      helptext="Details about Ranks of Talent">
      <it_separator/>
      </inputthing>
    <inputthing
      name="Not Ranked?"
      helptext="Talents are not ranked by default. Uncheck to treat talent as ranked.">
      <it_tagcheck group="TalentRank" tag="NotRanked" default="yes"/>
      </inputthing>
    <inputthing
      name="Rank 1?"
      helptext="Talent starts at Rank 1 (leave checked).">
      <it_tagcheck group="TalentRank" tag="Rank1" default="yes"/>
      </inputthing>
    <inputthing
      name="Rank 1 Summary"
      helptext="Specify a short summary for Rank 1 of Ranked talent.">
      <it_field field="talSummRank1"/>
      </inputthing>
    <inputthing
      name="Rank 2 Summary"
      helptext="Specify a short summary for Rank 2 of Ranked talent.">
      <it_field field="talSummRank2"/>
      </inputthing>
    <inputthing
      name="Rank 3 Summary"
      helptext="Specify a short summary for Rank 3 of Ranked talent.">
      <it_field field="talSummRank3"/>
      </inputthing>
    <inputthing
      name="Rank 4 Summary"
      helptext="Specify a short summary for Rank 4 of Ranked talent.">
      <it_field field="talSummRank4"/>
      </inputthing>
    <inputthing
      name="Rank 5 Summary"
      helptext="Specify a short summary for Rank 5 of Ranked talent.">
      <it_field field="talSummRank5"/>
      </inputthing>

<!-- These facets of special talents are not currently used for anything so they are commented out.
    <inputthing
      name="Minimum Value"
      helptext="Specify the minimum rating for the special ability (default is 0).">
      <it_field field="trtMinimum"/>
      </inputthing>
    <inputthing
      name="Maximum Value"
      helptext="Specify the maximum rating for the skill(default is 10).">
      <it_field field="trtMaximum"/>
