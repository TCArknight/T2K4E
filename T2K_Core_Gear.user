    <usesource source="srcT2K"/>
    <tag group="GearType" tag="ProtectiveGear"/>
    <tag group="GearSubType" tag="NA"/>
    </thing>
  <thing id="eqT2KPGChemAgtDet" name="Chemical Agent Detector" description="This small kit is used to assess whether a chemical agent is present in a hex and, if so, identify which agent. Requires one stretch of time to use." compset="Equipment">
    <fieldval field="grCost" value="75"/>
    <fieldval field="grReliability" value="1"/>
    <fieldval field="grWeight" value=".25"/>
    <usesource source="srcT2K"/>
    <tag group="GearType" tag="ProtectiveGear"/>
    <tag group="GearSubType" tag="NA"/>
    </thing>
  <thing id="eqT2KPGProtMask" name="Protective Mask" description="Used to reduce or counter the effects of chemical weapons. Removes the –3 modifier to STAMINA rolls for avoiding the effects (page 79), but also gives a –2 modifier to RECON and to STAMINA rolls made for marching." compset="Equipment">
    <fieldval field="grCost" value="150"/>
    <fieldval field="grReliability" value="1"/>
    <fieldval field="grWeight" value=".5"/>
    <usesource source="srcT2K"/>
    <tag group="GearType" tag="ProtectiveGear"/>
    <tag group="GearSubType" tag="NA"/>
    </thing>
  <thing id="eqT2KPGMOPPSuit" name="Hazmat/MOPP Suite" description="A suit, generally made of charcoal-impregnated water-resistant cloth in Western Europe and the US and waterproof plastic in Eastern Europe. Includes rubber gloves and boots and, when worn properly with a protective mask (above), protects you fully from most chemical weapons, even those that are lethal on contact with skin. Also offers some protection against radioactive contamination (page 80)." compset="Equipment">
    <fieldval field="grCost" value="500"/>
    <fieldval field="grReliability" value="1"/>
    <fieldval field="grWeight" value="1"/>
    <usesource source="srcT2K"/>
    <tag group="GearType" tag="ProtectiveGear"/>
    <tag group="GearSubType" tag="NA"/>
    </thing>
  <thing id="eqT2KPGRainGear" name="Rain Gear" description="Gives a +1 modifier to STAMINA rolls for avoiding the effects of chemical weapons." compset="Equipment">
    <fieldval field="grCost" value="25"/>
    <fieldval field="grReliability" value="1"/>
    <fieldval field="grWeight" value=".5"/>
