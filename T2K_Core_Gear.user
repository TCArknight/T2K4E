  <thing id="eqT2KTeleSight" name="Telescopic Sight" description="Gives you a +1 modifier if you aim as a slow action (+2 if you fire from a stable platform, such as a rest, sandbag, or bipod).  It is most often used by snipers and hunters" compset="Equipment">
    <fieldval field="grReliability" value="5"/>
    <fieldval field="grCost" value="100"/>
    <usesource source="srcT2K"/>
    <tag group="GearType" tag="WeaponGear"/>
    </thing>
  <thing id="eqT2KMagazine" name="Magazine" compset="Equipment" stacking="merge">
    <fieldval field="grReliability" value="5"/>
    <fieldval field="grWeight" value=".25"/>
    <usesource source="srcT2K"/>
    <tag group="thing" tag="holder"/>
    <tag group="GearType" tag="WeaponGear"/>
    <tag group="Equipment" tag="Magazine"/>
    <holdlimit>AmmoCat.Firearm</holdlimit>
    <gear>field[gearNet].value = field[gearWeight].value</gear>
    <eval phase="Final" priority="10000"><![CDATA[var heldName as string
	var heldQty as number
	
  if (gearcount = 0) then
    field[livename].text = field[name].text & " (Empty)"
  else
	foreach gear in this
	  heldQty = eachpick.field[stackQty].value
	  if (heldQty > 1) then
        heldName = splice(heldName, "(" & heldQty & "x) " & eachpick.field[name].text,", ")
	  else
	    heldName = splice(heldName, eachpick.field[name].text,", ")
		endif
      nexteach
    field[livename].text &= field[name].text & " (" & heldName & ")"
    endif]]></eval>
    </thing>
  <thing id="eqT2KBackpack" name="Backpack" description="Can be used to carry a number of encumbrance units equal to your STR die size, in addition to your combat gear. Carrying a backpack gives a –2 modifer to all MOBILITY rolls." compset="Equipment">
    <fieldval field="grCost" value="50"/>
    <fieldval field="grReliability" value="5"/>
    <usesource source="srcT2K"/>
    <tag group="Helper" tag="HasBackpack"/>
    <tag group="Helper" tag="IsBackpack"/>
    <tag group="Equipped" tag="Equippable"/>
    <tag group="GearType" tag="FieldGear"/>
    <tag group="thing" tag="holder_top"/>
    <tag group="GearSubType" tag="NA"/>
    <gear>herofield[acCarryBack].value = field[gearHeld].value</gear>
    <eval phase="Setup" priority="1000" index="1"><![CDATA[
	perform forward[Helper.HasBackpack]
	if (field[gearHeld].value <> 0) then
	  notify "Backpack holding items"
	  endif
	]]></eval>
    <eval phase="PreTraits" priority="5000" index="2"><![CDATA[if (field[grIsEquip].value <> 0) then
  #traitbonus[skT2KMobility] += -2
  endif]]></eval>
    <eval phase="Final" priority="1000" index="3"><![CDATA[
	if (field[gearHeld].value <> 0) then
	  ~ notify "Backpack holding items"
	  foreach gear in this
	    perform eachpick.assign[Helper.InBackpack]
		nexteach
	  endif
	]]></eval>
    </thing>
  <thing id="eqT2KAmmoBelt" name="Ammo Belt" compset="Equipment" stacking="merge">
    <fieldval field="grReliability" value="5"/>
    <fieldval field="grWeight" value="1"/>
    <usesource source="srcT2K"/>
    <tag group="thing" tag="holder"/>
    <tag group="GearType" tag="WeaponGear"/>
    <tag group="Equipment" tag="Magazine"/>
    <holdlimit>AmmoCat.Firearm</holdlimit>
    <gear>field[gearNet].value = field[gearWeight].value</gear>
    <eval phase="Render" priority="10000"><![CDATA[var heldName as string

if (gearcount = 0) then
  field[livename].text = field[name].text & " (Empty)"
else
  heldName = gearlist[",",component.Ammunition]
  field[livename].text &= field[name].text & " (" & heldName & ")"
  endif]]></eval>
    </thing>
